    "develop": "yarn clean & gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}' --cache --fix",
    "start": "yarn clean & gatsby develop",
